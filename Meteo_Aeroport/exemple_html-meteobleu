    || document.body.clientWidth;

    if (width > 600) {
     
        
        // Desktop ads
        var adSlot1, adSlot2;

        googletag.cmd.push(function () {
            var mapping1 = googletag.sizeMapping()
                .addSize([1200, 0], [300, 600], [300, 250], [160, 600], [120, 600], [240, 400], [250, 250], [200, 200], 'fluid')
